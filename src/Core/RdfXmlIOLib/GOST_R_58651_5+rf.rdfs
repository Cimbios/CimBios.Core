      <rdfs:label xml:lang="en">UsagePointLocation</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://iec.ch/TC57/CIM100#Location"/>
   </rdf:Description>
   <rdf:Description rdf:about="http://iec.ch/TC57/CIM100#UsagePointLocation.UsagePoints">
      <cims:AssociationUsed>Yes</cims:AssociationUsed>
      <cims:inverseRoleName rdf:resource="http://iec.ch/TC57/CIM100#UsagePoint.UsagePointLocation"/>
      <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..n"/>
      <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
      <rdfs:comment rdf:parseType="Literal">All usage points at this location.</rdfs:comment>
      <rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#UsagePointLocation"/>
      <rdfs:label xml:lang="en">UsagePoints</rdfs:label>
      <rdfs:range rdf:resource="http://iec.ch/TC57/CIM100#UsagePoint"/>
   </rdf:Description>
   <rdf:Description rdf:about="http://iec.ch/TC57/CIM100#UsagePointLocation.accessMethod">
      <cims:dataType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
      <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#attribute"/>
      <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Method for the service person to access this usage point location. For example, a description of where to obtain a key if the facility is unmanned and secured.</rdfs:comment>
      <rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#UsagePointLocation"/>
      <rdfs:label xml:lang="en">accessMethod</rdfs:label>
   </rdf:Description>
   <rdf:Description rdf:about="http://iec.ch/TC57/CIM100#UsagePointLocation.remark">
      <cims:dataType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
      <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#attribute"/>
      <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remarks about this location.</rdfs:comment>
      <rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#UsagePointLocation"/>
      <rdfs:label xml:lang="en">remark</rdfs:label>
   </rdf:Description>
   <rdf:Description rdf:about="http://iec.ch/TC57/CIM100#UsagePointLocation.siteAccessProblem">
      <cims:dataType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      <cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
      <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#attribute"/>
      <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
      <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Problems previously encountered when visiting or performing work at this location. Examples include: bad dog, violent customer, verbally abusive occupant, obstructions, safety hazards, etc.</rdfs:comment>
      <rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#UsagePointLocation"/>
      <rdfs:label xml:lang="en">siteAccessProblem</rdfs:label>
   </rdf:Description>
