		<rdfs:label>ratedCurrent</rdfs:label>
		<rdfs:comment>Rated current.</rdfs:comment>
		<rdfs:domain rdf:resource="#SwitchInfo"/>
		<cims:dataType rdf:resource="#CurrentFlow"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
	</rdf:Description>
	<rdf:Description rdf:about="#SwitchInfo.ratedInterruptingTime">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>ratedInterruptingTime</rdfs:label>
		<rdfs:comment>Switch rated interrupting time in seconds.</rdfs:comment>
		<rdfs:domain rdf:resource="#SwitchInfo"/>
		<cims:dataType rdf:resource="#Seconds"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
	</rdf:Description>
	<rdf:Description rdf:about="#SwitchInfo.ratedVoltage">
