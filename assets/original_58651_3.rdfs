	<rdf:Description rdf:about="#RatioTapChangerTablePoint.RatioTapChangerTable">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>RatioTapChangerTable</rdfs:label>
		<rdfs:comment>Table of this point.</rdfs:comment>
		<rdfs:domain rdf:resource="#RatioTapChangerTablePoint"/>
		<rdfs:range rdf:resource="#RatioTapChangerTable"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1"/>
		<cims:inverseRoleName rdf:resource="#RatioTapChangerTable.RatioTapChangerTablePoint"/>
	</rdf:Description>
	<rdf:Description rdf:about="#Reactance">
		<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
		<rdfs:label>#Reactance</rdfs:label>
		<rdfs:comment>Reactance (imaginary part of impedance), at rated frequency.</rdfs:comment>
		<cims:belongsToCategory rdf:resource="#Package_Domain"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#cimdatatype"/>
