		<rdfs:domain rdf:resource="#TemperatureDependentLimitPoint"/>
		<cims:dataType rdf:resource="#PerCent"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
	</rdf:Description>
	<rdf:Description rdf:about="#TemperatureDependentLimitPoint.temperature">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>temperature</rdfs:label>
		<rdfs:comment>The temperature of the table point.</rdfs:comment>
		<rdfs:domain rdf:resource="#TemperatureDependentLimitPoint"/>
		<cims:dataType rdf:resource="#Temperature"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
	</rdf:Description>
	<rdf:Description rdf:about="#TemperatureDependentLimitTable">
