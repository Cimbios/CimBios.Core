		<rdfs:comment>Points of this table.</rdfs:comment>
		<rdfs:domain rdf:resource="#RatioTapChangerTable"/>
		<rdfs:range rdf:resource="#RatioTapChangerTablePoint"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1..n"/>
		<cims:inverseRoleName rdf:resource="#RatioTapChangerTablePoint.RatioTapChangerTable"/>
	</rdf:Description>
	<rdf:Description rdf:about="#RatioTapChangerTablePoint">
		<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
		<rdfs:label>#RatioTapChangerTablePoint</rdfs:label>
		<rdfs:comment>Describes each tap step in the ratio tap changer tabular curve.</rdfs:comment>
		<cims:belongsToCategory rdf:resource="#Package_Wires"/>
		<rdfs:subClassOf rdf:resource="#TapChangerTablePoint"/>
