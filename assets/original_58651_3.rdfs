		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>AssetDatasheet</rdfs:label>
		<rdfs:comment>Datasheet information for this power system resource.</rdfs:comment>
		<rdfs:domain rdf:resource="#PowerSystemResource"/>
		<rdfs:range rdf:resource="#AssetInfo"/>
