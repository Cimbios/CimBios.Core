		<cims:inverseRoleName rdf:resource="#RatioTapChanger.RatioTapChangerTable"/>
	</rdf:Description>
	<rdf:Description rdf:about="#RatioTapChangerTable.RatioTapChangerTablePoint">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
