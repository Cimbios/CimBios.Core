		<rdfs:label>PowerSystemResource</rdfs:label>
		<rdfs:comment>The power system resource that contains the measurement.</rdfs:comment>
		<rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#Measurement"/>
		<rdfs:range rdf:resource="http://iec.ch/TC57/CIM100#PowerSystemResource"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
		<cims:inverseRoleName rdf:resource="http://iec.ch/TC57/CIM100#PowerSystemResource.Measurements"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#ofAggregate"/>
	</rdf:Description>
	<rdf:Description rdf:about="http://iec.ch/TC57/CIM100#Measurement.Terminal">
