	</rdf:Description>
	<rdf:Description rdf:about="http://iec.ch/TC57/CIM100#PowerTransformer.TransformerTanks">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>TransformerTanks</rdfs:label>
		<rdfs:comment>All transformers that belong to this bank.</rdfs:comment>
		<rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#PowerTransformer"/>
		<rdfs:range rdf:resource="http://iec.ch/TC57/CIM100#TransformerTank"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..n"/>
