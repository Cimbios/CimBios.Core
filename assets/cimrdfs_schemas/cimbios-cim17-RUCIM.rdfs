		<rdfs:comment>The tap step.</rdfs:comment>
		<rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#TapChangerTablePoint"/>
		<cims:dataType rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
