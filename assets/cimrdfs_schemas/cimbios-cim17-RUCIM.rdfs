		<cims:belongsToCategory rdf:resource="#Package_Domain"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#cimdatatype"/>
	</rdf:Description>
	<rdf:Description rdf:about="http://iec.ch/TC57/CIM100#Reading">
		<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
		<rdfs:label>Reading</rdfs:label>
		<rdfs:comment>Specific value measured by a meter or other asset, or calculated by a system. Each Reading is associated with a specific ReadingType.</rdfs:comment>
		<cims:belongsToCategory rdf:resource="#Package_Metering"/>
		<rdfs:subClassOf rdf:resource="http://iec.ch/TC57/CIM100#BaseReading"/>
	</rdf:Description>
	<rdf:Description rdf:about="http://iec.ch/TC57/CIM100#Reading.ReadingType">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>ReadingType</rdfs:label>
		<rdfs:comment>Type information for this reading value.</rdfs:comment>
		<rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#Reading"/>
		<rdfs:range rdf:resource="http://iec.ch/TC57/CIM100#ReadingType"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1"/>
		<cims:inverseRoleName rdf:resource="http://iec.ch/TC57/CIM100#ReadingType.Readings"/>
	</rdf:Description>
	<rdf:Description rdf:about="http://iec.ch/TC57/CIM100#Reading.reason">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>reason</rdfs:label>
		<rdfs:comment>Reason for this reading being taken.</rdfs:comment>
		<rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#Reading"/>
		<rdfs:range rdf:resource="http://iec.ch/TC57/CIM100#ReadingReasonKind"/>
