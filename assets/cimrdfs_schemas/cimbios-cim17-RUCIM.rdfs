		<cims:inverseRoleName rdf:resource="http://iec.ch/TC57/CIM100#ACLineSegmentPhase.ACLineSegment"/>
	</rdf:Description>
	<rdf:Description rdf:about="http://iec.ch/TC57/CIM100#ACLineSegment.PerLengthImpedance">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>PerLengthImpedance</rdfs:label>
		<rdfs:comment>Per-length impedance of this line segment.</rdfs:comment>
		<rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#ACLineSegment"/>
		<rdfs:range rdf:resource="http://iec.ch/TC57/CIM100#PerLengthImpedance"/>
